{
  "name": "focus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "chrome-types": "^0.1.329",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "@types/chrome": "^0.0.260",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:background && npm run build:app",
    "build:app": "PUBLIC_URL='./' react-scripts build && rm -rf extension/build && cp -r build extension/build && cp public/redirect.* extension/build/ && rm -rf build",
    "build:background": "tsc -p extension/background/tsconfig.json && cp extension/background/dist/background.js extension/background/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
